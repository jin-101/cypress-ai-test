{
  "auth": {
    "username": "${BROWSERSTACK_USERNAME}",
    "access_key": "${BROWSERSTACK_ACCESS_KEY}"
  },
  "run_settings": {
    "cypress_config_file": "cypress.config.js",
    "specs": "cypress/e2e/**/*.cy.{js,ts}",
    "parallels": 1,
    "npm_dependencies": {
      "cypress": "^15.0.0",
      "browserstack-cypress-cli": "^1.34.0",
      "dotenv": "^17.2.3",
      "express": "^5.1.0",
      "node-fetch": "^3.3.2"
    }
  },
  "connection_settings": {
    "local": true,
    "local_identifier": "ci-local-01"
  },
  "browsers": [
    {
      "browser": "chrome",
      "os": "osx",
      "versions": ["Ventura"]
    }
  ]
}
